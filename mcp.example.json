{
  "mcpServers": {
    "discord-py-self": {
      "command": "discord-selfbot-mcp",
      "env": {
        "DISCORD_TOKEN": "${DISCORD_TOKEN}",
        "GROQ_API_KEY": "${GROQ_API_KEY}",
        "CAPTCHA_PROXY": "${CAPTCHA_PROXY}",
        "RATE_LIMIT_ENABLED": "${RATE_LIMIT_ENABLED:-false}",
        "RATE_LIMIT_MESSAGES_PER_MINUTE": "${RATE_LIMIT_MESSAGES_PER_MINUTE:-10}",
        "RATE_LIMIT_MESSAGES_PER_SECOND": "${RATE_LIMIT_MESSAGES_PER_SECOND:-1}",
        "RATE_LIMIT_ACTIONS_PER_MINUTE": "${RATE_LIMIT_ACTIONS_PER_MINUTE:-5}",
        "RATE_LIMIT_COOLDOWN": "${RATE_LIMIT_COOLDOWN:-60}",
        "RATE_LIMIT_RESPECT_GLOBAL": "${RATE_LIMIT_RESPECT_GLOBAL:-true}",
        "TEMP_DIR": "${TEMP_DIR:-/tmp/hcaptcha}"
      }
    }
  }
}
